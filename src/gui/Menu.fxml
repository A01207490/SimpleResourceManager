<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="250.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MenuController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label onDragDetected="#switchToResources" text="Menu" />
            <Button maxWidth="100.0" mnemonicParsing="false" onAction="#switchToResources" text="Resources" />
            <Button maxWidth="100.0" mnemonicParsing="false" onAction="#switchToLandmarks" text="Landmarks" />
            <Button maxWidth="100.0" mnemonicParsing="false" onAction="#switchToLocations" text="Locations" />
            <Button maxWidth="100.0" mnemonicParsing="false" onAction="#switchToPaths" text="Routes" />
            <Button maxWidth="100.0" mnemonicParsing="false" onAction="#switchToQuery" text="Query" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
